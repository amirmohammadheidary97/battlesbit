{
  "name": "game-trade",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --fix . --ignore-pattern generated.ts",
    "lint": "yarn eslint",
    "lint:report": "eslint --ext .js,.jsx,.ts,.tsx . --output-file eslint_report.json --format json src",
    "preview": "vite preview",
    "gen:schema.json": "graphql-codegen",
    "gen:schema": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "format": "prettier --write ./src",
    "ionic": "ionic",
    "ionic:build": "yarn build",
    "ionic:serve": "yarn dev",
    "dev:android": "ionic capacitor run android -l --external",
    "dev:ios": "ionic capacitor run ios -l --external",
    "gradle:assembleRelease": "cd android && gradlew assembleRelease",
    "build:android": "ionic capacitor build android --no-open && npm run gradle:assembleRelease",
    "build:ios": "ionic capacitor build ios"
  },
  "keywords": [
    "ecosystem:ionic"
  ],
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/ios": "^6.1.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hamidfzm/eslint-config": "^0.10.0",
    "@ionic/react": "^8.2.7",
    "@mui/icons-material": "^6.1.1",
    "@mui/material": "6.0.0",
    "@types/react-swipeable-views": "^0.13.5",
    "@urql/core": "^5.0.5",
    "@urql/exchange-retry": "^1.3.0",
    "graphql": "^16.8.2",
    "lightweight-charts": "^4.2.1",
    "react": "^18.3.1",
    "react-awesome-animated-number": "^1.0.13",
    "react-confetti": "^6.1.0",
    "react-countdown": "^2.3.6",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.4",
    "react-otp-input": "^3.1.1",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "react-toastify": "^10.0.5",
    "react-use": "^17.5.1",
    "subscriptions-transport-ws": "^0.11.0",
    "swiper": "^11.1.15",
    "urql": "^4.1.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.2",
    "@chromatic-com/storybook": "^1.6.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.3",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-interactions": "^8.1.1",
    "@storybook/addon-links": "^8.1.1",
    "@storybook/addon-onboarding": "^8.1.1",
    "@storybook/blocks": "^8.1.1",
    "@storybook/react": "^8.1.1",
    "@storybook/react-vite": "^8.1.1",
    "@storybook/test": "^8.1.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@urql/devtools": "^2.0.3",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-storybook": "^0.8.0",
    "graphql-tag": "^2.12.6",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.0",
    "storybook": "^8.1.1",
    "typescript": "~5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-pwa": "^0.20.1",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.6.0",
    "workbox-build": "^7.1.1",
    "workbox-window": "^7.1.0"
  },
  "resolutions": {
    "**/react": "18.3.1",
    "**/react-dom": "18.3.1"
  }
}
